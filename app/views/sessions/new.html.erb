<h1>Sessions#new</h1>
<h1>Log in</h1>
<%= form_tag(sessions_path) do %>
<div class="field">
  <%= label_tag :username %>
  <%= text_field_tag :username%>
</div>

<div class="field">
  <%= label_tag :password_digest %>
  <%= text_field_tag :password_digest%>

<div class="form-group">
  <%= submit_tag "Login" %>
</div>
<% end %>
